{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Js",
      "type": "pwa-node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\dist\\index.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "name": "Current TS File",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}\\node_modules\\ts-node\\dist\\bin.js",
      "args": ["${relativeFile}"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
